<!--pages/mine/mine.wxml-->
<view class="head">
  <image src="/images/img/it.jpg"></image>
</view>
<view class="foot">
  <view bindtap="toProfile" class="div">
    <image class="top" src="/images/icon/number.png" />
    <text>个人信息</text>
  </view>
  <view bindtap="toFileshow" class="div">
    <image class="top" src="/images/icon/profile.png" />
    <text>我的简历</text>
  </view>
  <view bindtap="toMyJob" class="div">
    <image class="top" src="/images/icon/already.png" />
    <text>我的投递</text>
  </view>
  <view bindtap="toAnswerShow" class="div">
    <image class="top" src="/images/icon/answer.png" />
    <text>解答区</text>
  </view>
  <view class="div">
    <image class="top" src="/images/icon/help.png" />
    <text>帮助与反馈</text>
  </view>
</view>
